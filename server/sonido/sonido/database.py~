from pymongo import MongoClient

client = MongoClient("mongodb://sshi:1@troup.mongohq.com:10070/sonido")
db = client.sonido
collection = db.mapdata

def returnAll():
    allThings = collection.find()
    things = []
    for current in allThings:
        x = [current['long'],current['lat'],current['amp'],current['freq']]
        if things == None:
            things = [x]
            things.append(x)
    return things
